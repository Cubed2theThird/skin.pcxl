<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- Music -->
	<backgroundcolor>$VAR[BackgroundColor]</backgroundcolor>
	<defaultcontrol always="true">50</defaultcontrol>
	<menucontrol>3000</menucontrol>
	<onload>Skin.SetString(snapwindow, music)</onload>
	<onload>Skin.SetString(snapico, home/snap/music.jpg,0)</onload>
	<onload>Skin.SetString(snapname,Music)</onload>

	<views>50,522,523,531</views>
	<controls>
		<!-- Hidden controls -->
		<control type="button" id="3">
			<include>HiddenControl</include>
		</control>
		<control type="togglebutton" id="4">
			<usealttexture>Container.SortDirection(Descending)</usealttexture>
			<include>HiddenControl</include>
		</control>
		<control type="button" id="8">
			<include>HiddenControl</include>
		</control>
		<control type="button" id="16">
			<include>HiddenControl</include>
		</control>
		<control type="edit" id="19">
			<include>HiddenControl</include>
		</control>
		<control type="togglebutton" id="20">
			<usealttexture>Library.IsScanningVideo</usealttexture>
			<include>HiddenControl</include>
		</control>
		
		<control type="group">
			<animation type="Conditional" condition="Window.IsActive(contextmenu)">
				<effect type="zoom" center="auto" start="100" end="95" time="200" tween="cubic" easing="out"/>
			</animation>
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="$VAR[DiffuseColor]">views/white_50.png</texture>
			</control>
		</control>

		<!-- Now Playing -->
		<include>NowPlayingWindow</include>
		<!-- Main group -->
		<control type="group">
			<include>WindowFadeAnimation</include>
			<include>OptionsHideAnimation</include>
			<!-- Views -->
			<include>Viewtype_50_List</include>
			<include>Viewtype_522_ShowcaseSquare</include>
			<include>Viewtype_523_ShowcaseSquareLarge</include>
			<include>Viewtype_531_WallSquare</include>
			<!-- Now Playing Music -->
			<include>PlayingMusicWindow</include>
			<!-- ScrollBars -->
			<include>ScrollBars</include>
		</control>
		<!-- Options -->
		<control type="image">
			<include>VisibleFadeAnimation</include>
			<include>FullscreenDimensions</include>
			<texture colordiffuse="$VAR[SecondaryColor]">views/white_70.png</texture>
			<visible>Control.HasFocus(3000)</visible>
		</control>
		<control type="fixedlist" id="3000">
			<left>1030</left>
			<top>144</top>
			<width>1920</width>
			<height>896</height>
			<onup>50</onup>
			<ondown>50</ondown>
			<onback>50</onback>
			<onleft condition="String.IsEmpty(Container(3000).ListItem.Property(ActionLeft))">50</onleft>
			<onright condition="String.IsEmpty(Container(3000).ListItem.Property(ActionRight))">50</onright>
			<onleft condition="!String.IsEmpty(Container(3000).ListItem.Property(ActionLeft))">$INFO[Container(3000).ListItem.Property(ActionLeft)]</onleft>
			<onright condition="!String.IsEmpty(Container(3000).ListItem.Property(ActionRight))">$INFO[Container(3000).ListItem.Property(ActionRight)]</onright>
			<movement>6</movement>
			<focusposition>6</focusposition>
			<orientation>vertical</orientation>
			<viewtype label="535">list</viewtype>
			<defaultcontrol always="true">0</defaultcontrol>
			<scrolltime tween="sine" easing="inout">200</scrolltime>
			<visible allowhiddenfocus="true">Control.HasFocus(3000)</visible>
			<include>OptionsAnimation</include>
			<itemlayout height="59" width="1920">
				<control type="label">
					<width>1920</width>
					<height>59</height>
					<font>Thin48</font>
					<textcolor>$VAR[TextColor]</textcolor>
					<label>$INFO[ListItem.Label2]</label>
				</control>
			</itemlayout>
			<focusedlayout height="70" width="1920">
