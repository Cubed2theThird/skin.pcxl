			<itemlayout width="80" height="80">
				<control type="image">
					<width>80</width>
					<height>80</height>
					<texture>$INFO[ListItem.Icon]</texture>
				</control>
			</itemlayout>
			<focusedlayout width="80" height="80">
				<control type="image">
					<width>80</width>
					<height>80</height>
					<animation type="Focus">
						<effect type="slide" center="auto" start="-200" end="0" time="200" tween="cubic" easing="inout"/>
						<effect type="fade" center="auto" start="0" end="200" time="200" tween="cubic" easing="inout"/>
					</animation>
					<texture colordiffuse="$VAR[SecondaryColor]">color_white.png</texture>
				</control>
				<control type="image">
					<width>80</width>
					<height>80</height>
					<texture>$INFO[ListItem.Icon]</texture>
				</control>
			</focusedlayout>
			<content>
				<item id="1">
					<label fallback="31450">Home</label>
					<thumb>dialogs/charms/charm_home.png</thumb>
				</item>
				<item id="2">
					<label fallback="31450">Fullscreen</label>
					<thumb>dialogs/charms/charm_video.png</thumb>
					<visible>Player.HasMedia</visible>
					<onclick>Dialog.Close(all,true)</onclick>
					<onclick>Fullscreen</onclick>
				</item>
				<item id="5">
					<label fallback="31450">Power</label>
					<thumb>dialogs/charms/charm_shutdown.png</thumb>
				</item>
			</content>
