	<defaultcontrol always="true">1</defaultcontrol>
	<onload condition="Window.IsActive(9085)">Dialog.Close(all,true)</onload>
	<controls>

		<control type="group">
			<control type="image">
				<include>FullscreenDimensions</include>
				<texture colordiffuse="$VAR[BackgroundColor]">views/white_50.png</texture>
			</control>

			<control type="image">
				<centerleft>600</centerleft>
				<centertop>50%</centertop>
				<width>80</width>
				<height>50%</height>
				<texture colordiffuse="$VAR[BackgroundColor]">color_white.png</texture>
			</control>
		</control>
		
		<control type="list" id="1">
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>800</width>
			<height>50%</height>
			<scrolltime>0</scrolltime>
			<pagecontrol>2</pagecontrol>
			<viewtype>list</viewtype>
			<onright condition="Container(1).HasFocus(1)">2</onright>
			<onright condition="Container(1).HasFocus(2)">210</onright>
			<onright condition="Container(1).HasFocus(5)">510</onright>
